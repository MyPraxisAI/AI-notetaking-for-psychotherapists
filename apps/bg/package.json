{
  "name": "bg",
  "version": "1.0.0",
  "description": "MyPraxis Background Worker",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:docker:up": "docker-compose up dev-ts",
    "dev:docker:build": "docker-compose build dev-ts",
    "dev:docker:down": "docker-compose down dev-ts",
    "prod:docker:test": "docker-compose up --build prod-test",
    "docker:build": "docker build --target prod -t mypraxis/bg-worker:latest .",
    "docker:push:production": "AWS_PROFILE=mypraxis-terraform scripts/push-to-ecr.sh production",
    "deploy:production": "pnpm run docker:push:production && cd terraform && ./deploy.sh production apply",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@langchain/openai": "0.5.5",
    "@supabase/supabase-js": "2.49.1",
    "aws-sdk": "2.1692.0",
    "dotenv": "16.4.7",
    "langchain": "0.3.22",
    "openai": "4.96.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^22.13.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
