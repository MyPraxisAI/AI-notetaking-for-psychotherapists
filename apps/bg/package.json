{
  "name": "bg",
  "version": "1.0.0",
  "description": "MyPraxis Background Worker",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "bgdev": "pnpm run prod:docker:test",
    "prod:docker:test": "docker-compose up --build prod-test",
    "docker:build": "docker build --target prod -t mypraxis/bg-worker:latest .",
    "docker:push:production": "AWS_PROFILE=mypraxis-terraform scripts/push-to-ecr.sh production",
    "terraform:apply:production": "cd terraform && ./deploy.sh production apply",
    "deploy:production": "pnpm run docker:push:production && pnpm run terraform:apply:production",
    "clear-queue": "node scripts/clear-sqs-queue.js",
    "check-yandex-transcription": "tsx src/scripts/check-yandex-transcription.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.803.0",
    "@kit/shared-common": "workspace:*",
    "@kit/web-bg-common": "workspace:*",
    "@langchain/google-genai": "^0.2.4",
    "@langchain/openai": "^0.5.5",
    "@sentry/node": "^7.120.3",
    "@supabase/supabase-js": "2.49.1",
    "aws-sdk": "2.1692.0",
    "dotenv": "16.4.7",
    "js-tiktoken": "^1.0.19",
    "langchain": "^0.3.22",
    "nunjucks": "^3.2.4",
    "openai": "4.96.0",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@sentry/types": "^7.107.0",
    "@types/aws-sdk": "2.7.0",
    "@types/dotenv": "8.2.0",
    "@types/node": "^22.13.4",
    "dotenv-cli": "^8.0.0",
    "ts-node": "10.9.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
